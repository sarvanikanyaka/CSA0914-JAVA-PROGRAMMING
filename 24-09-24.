### 3. Sum of Digits in an Integer\par
\par
#### Pseudo Code\par
\par
START\par
FUNCTION main\par
    PRINT "Input an integer between 0 and 1000:"\par
    READ number\par
    sum = 0\par
    WHILE number > 0\par
        sum = sum + (number % 10)\par
        number = number / 10\par
    PRINT "The sum of all digits is " + sum\par
END FUNCTION\par
END\par
\par
\par
#### Java Code\par
\par
import java.util.Scanner;\par
\par
public class SumOfDigits \{\par
    public static void main(String[] args) \{\par
        Scanner scanner = new Scanner(System.in);\par
        System.out.print("Input an integer between 0 and 1000: ");\par
        int number = scanner.nextInt();\par
        int sum = 0;\par
\par
        while (number > 0) \{\par
            sum += number % 10;\par
            number /= 10;\par
        \}\par
        System.out.println("The sum of all digits is " + sum);\par
        scanner.close();\par
    \}\par
\}\par
\par
