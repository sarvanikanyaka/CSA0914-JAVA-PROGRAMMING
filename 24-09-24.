### 9. Integer Operations\par
\par
#### Pseudo Code\par
\par
START\par
FUNCTION main\par
    PRINT "Input 1st integer:"\par
    READ num1\par
    PRINT "Input 2nd integer:"\par
    READ num2\par
    sum = num1 + num2\par
    difference = num1 - num2\par
    product = num1 * num2\par
    average = sum / 2.0\par
    distance = ABS(difference)\par
    max = MAX(num1, num2)\par
    min = MIN(num1, num2)\par
    PRINT sum, difference, product, average, distance, max, min\par
END FUNCTION\par
END\par
\par
\par
#### Java Code\par
\par
import java.util.Scanner;\par
\par
public class IntegerOperations \{\par
    public static void main(String[] args) \{\par
        Scanner scanner = new Scanner(System.in);\par
        System.out.print("Input 1st integer: ");\par
        int num1 = scanner.nextInt();\par
        System.out.print("Input 2nd integer: ");\par
        int num2 = scanner.nextInt();\par
\par
        int sum = num1 + num2;\par
        int difference = num1 - num2;\par
        int\par
\par
 product = num1 * num2;\par
        double average = sum / 2.0;\par
        int distance = Math.abs(difference);\par
        int max = Math.max(num1, num2);\par
        int min = Math.min(num1, num2);\par
\par
        System.out.printf("Sum of two integers: %d%n", sum);\par
        System.out.printf("Difference of two integers: %d%n", difference);\par
        System.out.printf("Product of two integers: %d%n", product);\par
        System.out.printf("Average of two integers: %.2f%n", average);\par
        System.out.printf("Distance of two integers: %d%n", distance);\par
        System.out.printf("Max integer: %d%n", max);\par
        System.out.printf("Min integer: %d%n", min);\par
        scanner.close();\par
    \}\par
\}\par
\par
