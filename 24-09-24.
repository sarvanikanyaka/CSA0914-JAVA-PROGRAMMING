### 4. Minutes to Years and Days Conversion\par
\par
#### Pseudo Code\par
\par
START\par
FUNCTION main\par
    PRINT "Input the number of minutes:"\par
    READ minutes\par
    years = minutes / (60 * 24 * 365)\par
    days = (minutes % (60 * 24 * 365)) / (60 * 24)\par
    PRINT minutes + " minutes is approximately " + years + " years and " + days + " days"\par
END FUNCTION\par
END\par
\par
\par
#### Java Code\par
\par
import java.util.Scanner;\par
\par
public class MinutesToYearsDays \{\par
    public static void main(String[] args) \{\par
        Scanner scanner = new Scanner(System.in);\par
        System.out.print("Input the number of minutes: ");\par
        long minutes = scanner.nextLong();\par
        long years = minutes / (60 * 24 * 365);\par
        long days = (minutes % (60 * 24 * 365)) / (60 * 24);\par
        System.out.printf("%d minutes is approximately %d years and %d days%n", minutes, years, days);\par
        scanner.close();\par
    \}\par
\}\par
