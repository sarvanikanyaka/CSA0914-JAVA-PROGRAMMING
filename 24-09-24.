### 1. Fahrenheit to Celsius Conversion\par
\par
#### Pseudo Code\par
\par
START\par
FUNCTION main\par
    PRINT "Input a degree in Fahrenheit:"\par
    READ fahrenheit\par
    celsius = (fahrenheit - 32) * 5 / 9\par
    PRINT fahrenheit + " degree Fahrenheit is equal to " + celsius + " in Celsius"\par
END FUNCTION\par
END\par
\par
\par
#### Java Code\par
\par
import java.util.Scanner;\par
\par
public class FahrenheitToCelsius \{\par
    public static void main(String[] args) \{\par
        Scanner scanner = new Scanner(System.in);\par
        System.out.print("Input a degree in Fahrenheit: ");\par
        double fahrenheit = scanner.nextDouble();\par
        double celsius = (fahrenheit - 32) * 5 / 9;\par
        System.out.printf("%.1f degree Fahrenheit is equal to %.1f in Celsius%n", fahrenheit, celsius);\par
        scanner.close();\par
    \}\par
\}\par
\par
