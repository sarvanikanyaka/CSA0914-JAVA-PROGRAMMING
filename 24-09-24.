### 6. Body Mass Index (BMI) Calculation\par
\par
#### Pseudo Code\par
\par
START\par
FUNCTION main\par
    PRINT "Input weight in pounds:"\par
    READ weight\par
    PRINT "Input height in inches:"\par
    READ height\par
    bmi = (weight / (height * height)) * 703\par
    PRINT "Body Mass Index is " + bmi\par
END FUNCTION\par
END\par
\par
\par
#### Java Code\par
\par
import java.util.Scanner;\par
\par
public class BMICalculator \{\par
    public static void main(String[] args) \{\par
        Scanner scanner = new Scanner(System.in);\par
        System.out.print("Input weight in pounds: ");\par
        double weight = scanner.nextDouble();\par
        System.out.print("Input height in inches: ");\par
        double height = scanner.nextDouble();\par
        double bmi = (weight / (height * height)) * 703;\par
        System.out.println("Body Mass Index is " + bmi);\par
        scanner.close();\par
    \}\par
\}\par
