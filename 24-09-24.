### 7. Speed Calculation\par
\par
#### Pseudo Code\par
\par
START\par
FUNCTION main\par
    PRINT "Input distance in meters:"\par
    READ distance\par
    PRINT "Input hour:"\par
    READ hours\par
    PRINT "Input minutes:"\par
    READ minutes\par
    PRINT "Input seconds:"\par
    READ seconds\par
    totalTime = hours * 3600 + minutes * 60 + seconds\par
    speedMps = distance / totalTime\par
    speedKmph = speedMps * 3.6\par
    speedMph = speedMps * 2.23694\par
    PRINT speedMps, speedKmph, speedMph\par
END FUNCTION\par
END\par
\par
\par
#### Java Code\par
\par
import java.util.Scanner;\par
\par
public class SpeedCalculator \{\par
    public static void main(String[] args) \{\par
        Scanner scanner = new Scanner(System.in);\par
        System.out.print("Input distance in meters: ");\par
        double distance = scanner.nextDouble();\par
        System.out.print("Input hour: ");\par
        int hours = scanner.nextInt();\par
        System.out.print("Input minutes: ");\par
        int minutes = scanner.nextInt();\par
        System.out.print("Input seconds: ");\par
        int seconds = scanner.nextInt();\par
\par
        double totalTime = hours * 3600 + minutes * 60 + seconds;\par
        double speedMps = distance / totalTime;\par
        double speedKmph = speedMps * 3.6;\par
        double speedMph = speedMps * 2.23694;\par
\par
        System.out.printf("Your speed in meters/second is %.8f%n", speedMps);\par
        System.out.printf("Your speed in km/h is %.8f%n", speedKmph);\par
        System.out.printf("Your speed in miles/h is %.8f%n", speedMph);\par
        scanner.close();\par
    \}\par
\}\par
