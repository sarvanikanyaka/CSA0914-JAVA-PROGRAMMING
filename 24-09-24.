### 8. Square, Cube, and Fourth Power\par
\par
#### Pseudo Code\par
\par
START\par
FUNCTION main\par
    PRINT "Enter a number:"\par
    READ number\par
    square = number * number\par
    cube = number * number * number\par
    fourthPower = square * square\par
    PRINT square, cube, fourthPower\par
END FUNCTION\par
END\par
\par
\par
#### Java Code\par
\par
import java.util.Scanner;\par
\par
public class PowersCalculator \{\par
    public static void main(String[] args) \{\par
        Scanner scanner = new Scanner(System.in);\par
        System.out.print("Enter a number: ");\par
        double number = scanner.nextDouble();\par
        double square = number * number;\par
        double cube = number * number * number;\par
        double fourthPower = square * square;\par
\par
        System.out.printf("Square: %.2f%n", square);\par
        System.out.printf("Cube: %.2f%n", cube);\par
        System.out.printf("Fourth power: %.2f%n", fourthPower);\par
        scanner.close();\par
    \}\par
\}\par
