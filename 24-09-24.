### 2. Inches to Meters Conversion\par
\par
#### Pseudo Code\par
\par
START\par
FUNCTION main\par
    PRINT "Input a value for inch:"\par
    READ inch\par
    meter = inch * 0.0254\par
    PRINT inch + " inch is " + meter + " meters"\par
END FUNCTION\par
END\par
\par
\par
#### Java Code\par
\par
import java.util.Scanner;\par
\par
public class InchesToMeters \{\par
    public static void main(String[] args) \{\par
        Scanner scanner = new Scanner(System.in);\par
        System.out.print("Input a value for inch: ");\par
        double inch = scanner.nextDouble();\par
        double meter = inch * 0.0254;\par
        System.out.printf("%.1f inch is %.1f meters%n", inch, meter);\par
        scanner.close();\par
    \}\par
\}\par
