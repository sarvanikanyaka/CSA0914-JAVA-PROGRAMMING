### 5. Current Time in GMT\par
\par
#### Pseudo Code\par
\par
START\par
FUNCTION main\par
    PRINT "Input the time zone offset to GMT:"\par
    READ offset\par
    currentTime = GMT time + offset\par
    PRINT "Current time is " + currentTime\par
END FUNCTION\par
END\par
\par
\par
#### Java Code\par
\par
import java.util.Calendar;\par
import java.util.Scanner;\par
\par
public class CurrentTimeInGMT \{\par
    public static void main(String[] args) \{\par
        Scanner scanner = new Scanner(System.in);\par
        System.out.print("Input the time zone offset to GMT: ");\par
        int offset = scanner.nextInt();\par
\par
        Calendar calendar = Calendar.getInstance();\par
        calendar.add(Calendar.HOUR_OF_DAY, offset);\par
        int hours = calendar.get(Calendar.HOUR_OF_DAY);\par
        int minutes = calendar.get(Calendar.MINUTE);\par
        int seconds = calendar.get(Calendar.SECOND);\par
\par
        System.out.printf("Current time is %02d:%02d:%02d%n", hours, minutes, seconds);\par
        scanner.close();\par
    \}\par
\}\par
